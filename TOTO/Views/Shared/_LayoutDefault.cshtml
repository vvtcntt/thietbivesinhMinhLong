</head>
<body>
    @Html.Raw(ViewBag.h1)
    <header>
        @{Html.RenderAction("ControlHeader", "Header");}
    </header>
    <div class="Clear"></div>
    <section>
        <div id="Main">
          @RenderBody()
        </div>
        </section>
 
    <footer>
        <script src="~/Content/Display/Js/Adw.js"></script>

        @{Html.RenderAction("ControlFootter", "Footter");}
        <!--    Quảng cáo-->
        @if(Request.Browser.IsMobileDevice)
        { }
        else
        {
            Html.RenderAction("AdwLeftRight", "Default");
            Html.RenderAction("AdwLeftExt", "Default");
           
            Html.RenderAction("Popup", "Default");
            
        }
      
        
        <script>
            document.write("<script type='text/javascript' language='javascript'>MainContentW = 1100;LeftBannerW = 130;RightBannerW = 130;LeftAdjust = 5;RightAdjust = 5;TopAdjust = 140;ShowAdDiv();window.onresize=ShowAdDiv;;<\/script>");
        </script>
    </footer>
    <script src="~/Scripts/jquery-2.1.4.js"></script>
 
    <link href="~/Content/PagedList.css" rel="stylesheet" />
    <script src="~/Content/Display/Js/jquery.mmenu.min.all.js"></script>
    <script src="~/Content/Display/Js/jquery.prettyPhoto.js"></script>
    <script src="~/Content/Display/Js/PopupDetailProduct.js"></script>
    <script src="~/Content/Display/Js/Scroll.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
     <script src="~/Content/Display/Js/jquery.popup.js"></script>
    <script type="text/javascript">
        $(function () {
            $('nav#menu').mmenu({
                extensions: ['effect-slide', 'pageshadow'],
                header: true,
                searchfield: true,
                counters: true,
                footer: {
                    add: true,
                }
            });
        });
    </script>
    <script type="text/javascript">
        function hide_float_left() {
            var content = document.getElementById('float_content_left');
            var hide = document.getElementById('hide_float_left');
            if (content.style.display == "none")
            { content.style.display = "block"; hide.innerHTML = '<a href="javascript:hide_float_left()">Tắt quảng cáo [X]</a>'; }
            else {
                content.style.display = "none"; hide.innerHTML = '<a href="javascript:hide_float_left()">Xem quảng cáo...</a>';
            }
        }
    </script>
    <script type="text/javascript">
        $(window).load(function () {
            if (document.cookie.indexOf("adf") == -1) {
                document.cookie = "popunder1=adf";
                $('#myModal').linhnguyen($('#myModal').data());
            }
        });
    </script>

     
    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: '1486305081684176',
                xfbml: true,
                version: 'v2.4'
            });
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

    <script type="text/javascript" src="https://apis.google.com/js/plusone.js"></script>
</body>
</html>
